{
  "name": "fixers-in-greece-tailwind",
  "version": "1.0.0",
  "description": "Tailwind CSS setup for Fixers in Greece website",
  "main": "index.js",
  "scripts": {
    "build-css": "tailwindcss -i ./src/input.css -o ./dist/output.css --watch",
    "build-css-prod": "tailwindcss -i ./src/input.css -o ./dist/output.css --minify",
    "dev": "node build-optimized.js --watch --verbose",
    "build": "node build-optimized.js --production --minify",
    "build-stats": "node build-optimized.js --production --minify --verbose",
    "watch": "node build-optimized.js --watch",
    "analyze": "tailwindcss -i ./src/input.css -o ./dist/output.css --minify && ls -la dist/",
    "clean": "rm -rf dist/ && mkdir dist"
  },
  "keywords": [
    "tailwindcss",
    "greece",
    "film-production",
    "website"
  ],
  "author": "Fixers in Greece",
  "license": "MIT",
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "tailwindcss": "^3.4.0"
  },
  "dependencies": {}
}
